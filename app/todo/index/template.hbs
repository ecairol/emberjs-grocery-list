<fieldset>
  <legend>Grocery List</legend>
  {{search-bar 
    keywords=search
    debounce=100
    search=(action "search")}}
  <hr>
  {{todo-form newItem=(action "newItem")}}
  {{todo-list 
      items=filtered 
      delete=(action "deleteItem") 
      bulkDelete=(action "deleteItems")
      done=(action "markAsDone")}}
  {{outlet}}
</fieldset>
